    "st": "start-storybook -s ./static -p 6006",
    "tests": "jest --config jest.config.json",
    "jobs": "node --require babel-register ./initializers/server/jobs/index.js",
    "parse:mixpanel": "node --require babel-register ./initializers/server/services/mixpanelCreateFile.js"
